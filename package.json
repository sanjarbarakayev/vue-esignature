{
  "name": "@sanjarbarakayev/vue-esignature",
  "version": "1.3.0",
  "description": "Vue 3 plugin for E-IMZO electronic digital signature (EDS) integration - Uzbekistan's national digital signature system",
  "author": "Sanjar Barakayev",
  "license": "MIT",
  "homepage": "https://github.com/sanjarbarakayev/vue-esignature#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanjarbarakayev/vue-esignature.git"
  },
  "bugs": {
    "url": "https://github.com/sanjarbarakayev/vue-esignature/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "e-imzo",
    "eimzo",
    "digital-signature",
    "electronic-signature",
    "eds",
    "pkcs7",
    "uzbekistan",
    "cryptography",
    "signing",
    "certificate"
  ],
  "type": "module",
  "main": "./dist/vue-esignature.umd.cjs",
  "module": "./dist/vue-esignature.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-esignature.js",
      "require": "./dist/vue-esignature.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "playground": "vite --config playground/vite.config.ts",
    "playground:build": "vite build --config playground/vite.config.ts --outDir playground/dist",
    "playground:preview": "vite preview --config playground/vite.config.ts --outDir playground/dist",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.6",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^2.0.0",
    "happy-dom": "^15.0.0",
    "terser": "^5.46.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitepress": "^1.5.0",
    "vitest": "^2.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^3.2.4",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
